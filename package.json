{
  "name": "cocos-pnp",
  "version": "1.3.7",
  "description": "Ads Playable for Cocos",
  "main": "index.js",
  "scripts": {
    "commit": "cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:core": "pnpm -F playable-adapter-core build",
    "build:2x": "pnpm run build:core && pnpm -F playable-ads-adapter build:2x",
    "build:3x": "pnpm run build:core && pnpm -F playable-ads-adapter build:3x",
    "build": "pnpm run build:core && pnpm -F playable-ads-adapter build:2x && pnpm -F playable-ads-adapter build:3x",
    "watch:2x": "pnpm -F playable-ads-adapter watch:2x",
    "watch:3x": "pnpm -F playable-ads-adapter watch:3x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ppgee/cocos-pnp.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "keywords": [
    "cocos",
    "ads-playable",
    "facebook",
    "unity",
    "google",
    "tiktok"
  ],
  "author": "ppgee",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1"
  }
}